{
	"name": "epic-launcher",
	"type": "module",
	"version": "1.0.0",
	"bin": {
		"epic-launcher": "./src/bin.js"
	},
	"scripts": {
		"dev": "npx --yes cross-env NODE_ENV=development npx --yes nodemon --es-module-specifier-resolution=node --inspect ./src/index",
		"start": "node --es-module-specifier-resolution=node ./src/index",
		"clean": "npx --yes rimraf node_modules transpiled package-lock.json"
	},
	"dependencies": {
		"ps-list": "^8.1.0"
	}
}